{
  "name": "collectxyz-web-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "typecheck": "tsc --project tsconfig.client.json --noEmit",
    "typecheck-watch": "tsc --project tsconfig.client.json --noEmit --watch --preserveWatchOutput",
    "concurrent-build": "concurrently \"npm run build-server\" \"npm run build-client\"",
    "build-server": "rm -rf ./private && webpack --config webpack.server.config.js",
    "build-client": "rm -rf ./public && webpack --config webpack.client.config.js --output-public-path /public/",
    "dev": "npm run client:dev & npm run server:dev",
    "client:dev": "webpack-dev-server --config webpack.dev.config.js",
    "server:dev": "npm run watch-server & npm run node-server:dev",
    "server:prod": "npm run node-server:prod",
    "webpack-help": "webpack watch --help",
    "watch-server": "webpack watch --config webpack.server.config.js",
    "node-server:dev": "NODE_ENV=development nodemon --delay 2 private/server.js",
    "node-server:prod": "NODE_ENV=production node private/server.js",
    "lint-all": "eslint './src/**/*.tsx' './src/**/*.ts' --fix"
  },
  "author": "xyz Nuisance",
